<div class="nested-fields">
	<div class="listing-form">
	  <%= f.label :name %>
	  <div class="max-starting-salary-input">
	    <%= f.text_field :name %>
	  </div>
	</div>

	<div class="listing-form">
	  <%= f.label :description %>
	  <div class="max-starting-salary-input">
	  	<%= f.text_area(:description, size: "20x5") %>
	  </div>
	</div>

	<div>
		<%= link_to_remove_association "-Remove", f %>
	</div>
</div>
